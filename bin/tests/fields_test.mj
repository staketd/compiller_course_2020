class Main {
    public static void main() {
        Test test = new Test();
        test.SetField(0);
        test.PrintField();
        test.SetField(1);
        test.PrintField();
    }
}

class Test {
    Integer field_;
    void SetField(Integer a) {
        field_ = a;
    }

    void PrintField() {
        print(field_);
    }
}