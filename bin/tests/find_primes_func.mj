Integer find_primes(b) {
    Integer a;
    a = 2;
    while (a < b) {
        Integer k;
        k = 2;
        Integer flag;
        flag = 0;
        while (k * k <= a) {
            if (a % k == 0) {
                flag = 1;
            }
            k = k + 1;
        }
        if (flag == 0) {
            print(a);
        }
        a = a + 1;
    }
}

Integer main() {
    find_primes(100);
}