class Main {
    public static void main() {
        Integer a = 2;
        Integer b = 0;
        read(b);
        while (a < b) {
            Integer k = 2;
            Integer flag = 0;
            while (k * k <= a) {
                if (a % k == 0) {
                    flag = 1;
                }
                k = k + 1;
            }
            if (flag == 0) {
                print(a);
            }
            a = a + 1;
        }
    }
}